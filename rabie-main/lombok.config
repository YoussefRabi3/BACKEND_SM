# Set the access level for generated getters and setters to 'public'
lombok.accessors.prefix = ""
lombok.accessors.chain = true

# Generate a logger field for each class
lombok.log.fieldName = "log"
lombok.log.fieldModifier = "private static final"

# Generate a builder class for each class
lombok.experimental.SuperBuilder.includeBuilderInEqualsAndHashCode = true
lombok.experimental.SuperBuilder.includeBuilderInToString = true
